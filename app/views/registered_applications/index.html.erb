<div class="abthm lpad15">
  <h5><%= link_to "Home", welcome_index_path, class:  "cfont font18 .lpad10" %></h5>
</div>
<div class="bgcenter ">
  <h1 class="ggggfont tpad15 lpad25">Current Registered Applications</h1>
<div>
<div class="row pad25">
  <h4 class="infoinfo tpad50">
    <div class="container pull-left">
      <h5><%= link_to "Add an App", new_registered_application_path, class: "pull-left lmarneg10 dfont font18 bmar25" %></h5>
    </div>
    <% @reg_apps.each do |reg_app| %>
    <%= link_to reg_app.name, registered_applications_path %>
    <h4>@reg_app.url</h4>
    <p>
      <%= @reg_apps.name %>
    </p>
    <% end %>
  </h4>
</div>
<div class="topline glyphist">
  <a class="glyphicon glyphicon-align-left glyphist"></a>
  <a class="glyphicon glyphicon-stats lmar25 rmar25 glyphist"></a>
  <a class="glyphicon glyphicon-align-right glyphist"></a>
</div>
